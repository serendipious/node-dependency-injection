{
  "name": "minjex",
  "version": "0.5.0",
  "description": "Minimal dependency injection for Node.js with TypeScript support",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.2",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "scripts": {
    "clean": "rm -rf ./{dist,node_modules,coverage}",
    "build": "tsc",
    "compile": "npm run build",
    "test": "nyc mocha -r ts-node/register tst/test-*.ts",
    "test:watch": "nyc mocha -r ts-node/register --watch tst/test-*.ts",
    "test:coverage": "nyc mocha -r ts-node/register tst/test-*.ts --reporter=html",
    "pre-commit": "npm test",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/serendipious/node-dependency-injection"
  },
  "keywords": [
    "dependency",
    "injection"
  ],
  "author": "Ankit Kuwadekar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serendipious/node-dependency-injection/issues"
  },
  "homepage": "https://github.com/serendipious/node-dependency-injection"
}
